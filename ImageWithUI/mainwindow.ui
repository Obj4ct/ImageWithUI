/* QLineEdit 样式 */
QLineEdit {
    background-color: #ffffff; /* 设置背景颜色为白色 */
    border: 3px solid #ccc; /* 边框样式 */
    border-radius: 5px; /* 圆角半径 */
    padding: 5px; /* 内边距 */
    color: #333; /* 文本颜色 */
}

QLineEdit:focus {
    border-color: #007BFF; /* 获取焦点时的边框颜色 */
}


/* QMenu 样式 */
QMenu {
     background-color: #f0f0f0; /* 背景颜色 */

    border: 1px solid #ccc; /* 边框样式 */
}

QMenu::item {
    padding: 5px 20px; /* 内边距 */
}

QMenu::item:selected {
    background-color: #007BFF; /* 选中项的背景颜色 */
    color: white; /* 选中项的文本颜色 */
}

/* QAction 样式 */
QAction {
    background-color: transparent; /* 背景颜色透明 */
    color: #333; /* 文本颜色 */
}

QAction:hover {
    background-color: #e0e0e0; /* 鼠标悬停时的背景颜色 */
}

QAction:pressed {
    background-color: #d4d4d4; /* 按下时的背景颜色 */
}

/* QStatusBar 样式 */
QStatusBar {
    background-color: rgb(203, 223, 255);/* 背景颜色 */
    color: #333; /* 文本颜色 */
    border-top: 1px solid #ccc; /* 顶部边框样式 */
}

QStatusBar::item {
    border: none; /* 无边框 */
}
